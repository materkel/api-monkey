{
  "name": "api-monkey",
  "version": "0.2.2",
  "description": "Middleware for test simulations with multi layered Express APIs. Delay requests to specific API Endpoints and return custom Error codes.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir .",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register",
    "start": "./node_modules/.bin/babel src --out-dir . --watch"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "keywords": [],
  "author": "Matthias Fre√üdorf <mfressdorf@gmail.com> (http://mfressdorf.de)",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfressdorf/api-monkey"
  },
  "licenses": {
    "type": "MIT",
    "url": "https://github.com/mfressdorf/api-monkey/blob/master/LICENSE.md"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "express": "^4.13.4",
    "mocha": "^2.5.3",
    "request-promise": "^3.0.0",
    "supertest": "^1.2.0"
  }
}
